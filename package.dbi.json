{
    "name": "repo_svt",
    "version": "1.0.0",
    "description": "## Realizzazione di un progetto per il monitoraggio e la gestione di infrazioni di velocit√† dei veicoli su tratte autostradali. ",
    "main": "index.js",
    "scripts": {
      "build": "tsc",
      "start": "node dist/index.js",
      "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' --files src/index.ts",
      "lint": "eslint 'src/**/*.{js,ts}'",
      "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
      "prettier": "prettier --write 'src/**/*.{js,ts,json,md}'",
      "test": "jest --config jest.config.cjs",
      "migrate": "npx sequelize-cli db:migrate --config sequelize-cli.config.js",
      "seed": "npx sequelize-cli db:seed:all --config sequelize-cli.config.js",
      "migrate-and-seed": "npm run migrate && npm run seed",
      "seed:undo": "npx sequelize-cli db:seed:undo --config sequelize-cli.config.js",
      "seed:undo:all": "npx sequelize-cli db:seed:undo:all --config sequelize-cli.config.js",
      "seed:specific": "npx sequelize-cli db:seed --seed <nome_seeder> --config sequelize-cli.config.js"
    },
    "_moduleAliases": {
    "@": "dist"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
      "@types/sequelize": "^4.28.20",
      "date-fns": "^4.1.0",
      "dotenv": "^16.4.5",
      "module-alias": "^2.2.3",
      "morgan": "^1.10.0",
      "multer": "^1.4.5-lts.1",
      "mysql2": "^3.11.3",
      "sequelize": "^6.37.3",
      "util": "^0.12.5",
      "winston": "^3.14.2"
    },
    "devDependencies": {
      "@types/dotenv": "^6.1.1",
      "@types/mocha": "^10.0.8",
      "@types/morgan": "^1.9.9",
      "@types/multer": "^1.4.12",
      "@types/node": "^22.7.4",
      "nodemon": "^3.1.7",
      "prettier": "^3.3.3",
      "sequelize-cli": "^6.6.2",
      "ts-node": "^10.9.2"
    }
  }
  